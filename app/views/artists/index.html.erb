<p id="notice"><%= notice %></p>

<h1>Listing Artists</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Twitter url</th>
      <th>Facebook url</th>
      <th>Behance url</th>
      <th>Linkedin url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= artist.first_name %></td>
        <td><%= artist.last_name %></td>
        <td><%= artist.twitter_url %></td>
        <td><%= artist.facebook_url %></td>
        <td><%= artist.behance_url %></td>
        <td><%= artist.linkedin_url %></td>
        <td><%= link_to 'Show', artist %></td>
        <td><%= link_to 'Edit', edit_artist_path(artist) %></td>
        <td><%= link_to 'Destroy', artist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artist', new_artist_path %>
